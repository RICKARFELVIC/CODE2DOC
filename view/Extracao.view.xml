					<!--	<Text text="Status"/>-->
					<!--</Column>-->
				</columns>
				<items>
					<ColumnListItem type="Active">
						<cells>
							<Text text="001"/>
							<Text text="A0101"/>
							<Text text="1000"/>
							<Text text="A0101"/>
							<Text text="1000"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
				<MessageStrip id="msgTab" text="Selecione um documento na lista para adicionar objetos" showIcon="true" showCloseButton="true"
